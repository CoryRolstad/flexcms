if(document.querySelector("#current-contact-<%= escape_javascript(@contact.id.to_s) %>").childElementCount < 1)
    $("#current-contact-<%= escape_javascript(@contact.id.to_s) %>").html("<%= escape_javascript(render partial: 'contacts/show', locals: { contact: @contact } ) %>");

if($("#contact-edit-row-<%= escape_javascript(@contact.id.to_s) %>").hasClass("hidden"))
    $("#contact-edit-row-<%= escape_javascript(@contact.id.to_s) %>").removeClass("hidden");
else
    $("#contact-edit-row-<%= escape_javascript(@contact.id.to_s) %>").addClass("hidden");